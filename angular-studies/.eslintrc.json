{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "project": "./tsconfig.json"
  },
  "plugins": [
      "@typescript-eslint", "prettier"
  ],
  "rules": {
    "no-alert": "warn",
    "no-console": "warn",
    "import/prefer-default-export": "off"
  }
}
